{
  "name": "sicario-build-template",
  "version": "0.8.0",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Eung Jin Lee",
    "email": "undefinedist@gmail.com",
    "url": "https://undefinedist.github.io/sicario/"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "components",
    "undefinedist",
    "react",
    "react-component",
    "toolkit"
  ],
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "color": "^2.0.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^15.6.1",
    "react-dev-utils": "^4.0.0",
    "react-dom": "^15.6.1",
    "rebass": "^1.0.4",
    "style-loader": "0.18.2",
    "styled-components": "^2.1.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "homepage": "http://undefinedist.github.io/sicario/",
  "scripts": {
    "prestart": "yarn run gen:docs",
    "start": "npm-run-all --parallel gen:docs-watch start:docs",
    "start:docs": "node scripts/start.js",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "yarn gen:docs -- --watch",
    "build:docs": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prebuild:lib": "rimraf lib",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "build:lib": "npm-run-all --parallel build:commonjs build:css build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "predeploy:docs": "yarn build:docs",
    "deploy:docs": "gh-pages -d build",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "version:patch": "npm version patch && yarn build:lib && npm publish --prefix ./lib",
    "version:minor": "npm version minor && yarn build:lib"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": "commonjs"
        }
      ],
      "react-app"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "prettier",
      "prettier/flowtype",
      "prettier/react"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chokidar": "^1.7.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "gh-pages": "^1.0.0",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.6.1",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.6.0",
    "react-docgen": "^2.17.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1"
  }
}
